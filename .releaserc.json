{
  "branches": ["uat", "main",{
    "name": "dev", 
    "prerelease": true
  },{
    "name": "+([hH]otfix*)",
    "prerelease": true
  }],
  "ci": false,
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          { "type": "breaking", "release": "major" },
          { "type": "major", "release": "major" }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/github"
  ]
}